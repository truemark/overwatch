{
  "name": "overwatch-handlers",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prebuild": "prettier --check . && eslint .",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest run --coverage",
    "clean": "find ./src -type f \\( -name \"*.mjs\" -o -name \"*.d.mts\" -o -name \"*.map\" \\) -exec rm -f {} \\;",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.849.0",
    "@aws-sdk/client-firehose": "^3.848.0",
    "@aws-sdk/client-iam": "^3.848.0",
    "@aws-sdk/client-osis": "^3.848.0",
    "@aws-sdk/client-sqs": "^3.848.0",
    "@aws-sdk/client-ssm": "^3.849.0",
    "@aws-sdk/client-sts": "^3.848.0",
    "@aws-sdk/util-arn-parser": "^3.804.0",
    "@nr1e/logging": "^1.6.0",
    "@opensearch-project/opensearch": "^2.13.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "22.16.5",
    "aws-sdk-client-mock": "^1.0.0",
    "source-map-support": "^0.5.21",
    "@eslint/js": "^8.57.1",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "vitest": "^1.6.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^7.18.0"
  }
}
