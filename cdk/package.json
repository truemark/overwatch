    "aws-cdk": "^2.1021.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.206.0",
    "constructs": "^10.4.2",
